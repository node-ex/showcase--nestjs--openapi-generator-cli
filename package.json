{
  "name": "nestjs-template--basic",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "nest start --watch",
    "dev:debug": "nest start --debug --watch",
    "dev:client": "npm run swagger:json:generate && npm run swagger:docker:build && npm run swagger:client:generate && npm run swagger:client:transpile && npm run dev",
    "swagger:docker:build": "./scripts/docker/openapi-generator-cli/build.sh",
    "swagger:client:generate": "./scripts/docker/openapi-generator-cli/generate-client-sdk.sh",
    "swagger:client:transpile": "./scripts/docker/openapi-generator-cli/transpile-client-sdk.sh",
    "swagger:json:validate": "./scripts/docker/openapi-generator-cli/validate-spec.sh",
    "swagger:json:generate": "ts-node src/generate-swagger.ts",
    "build": "nest build",
    "start": "node dist/main",
    "lint": "npm run lint:types && npm run lint:eslint && npm run lint:prettier",
    "lint:fix": "npm run lint:types && npm run lint:eslint:fix && npm run lint:prettier:fix",
    "lint:types": "tsc --project ./tsconfig.json",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "lint:prettier:single": "prettier --list-different",
    "lint:eslint": "eslint --format visualstudio --ext .js,.mjs,.ts --report-unused-disable-directives --max-warnings 0 .",
    "lint:eslint:fix": "eslint --format visualstudio --ext .js,.mjs,.ts --report-unused-disable-directives --max-warnings 0 --fix .",
    "lint:eslint:single": "eslint"
  },
  "dependencies": {
    "@nestjs/common": "10.3.8",
    "@nestjs/core": "10.3.8",
    "@nestjs/platform-express": "^10.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/swagger": "7.3.1",
    "@openapitools/openapi-generator-cli": "2.13.4",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cpy-cli": "^5.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
